# Clawdbot Gateway connection (required)
CLAWDBOT_GATEWAY_URL=http://localhost:18789
CLAWDBOT_GATEWAY_TOKEN=your-gateway-token

# API key for incoming requests (required for production)
# Generate with: openssl rand -hex 24
# If not set, endpoint is open (not recommended)
VOICE_BRIDGE_API_KEY=your-api-key-here
